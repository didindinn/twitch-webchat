  "homepage": "https://github.com/talmobi/twitch-webchat#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talmobi/twitch-webchat.git"
  },
  "bugs": {
    "url": "https://github.com/talmobi/twitch-webchat/issues"
  },
  "dependencies": {
    "jquery": "2.2.2",
    "phantomjs-prebuilt": "2.1.7"
  },
  "devDependencies": {
    "express": "^4.13.4",
    "mocha": "^2.4.5",
    "request": "^2.69.0",
    "socket.io": "^1.4.5"
